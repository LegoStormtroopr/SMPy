#/bin/bash
mkdir -p surveys ;
mv ~/dev/surveyAutomation/surveyAutomation/src/system/surveys/*.question ./surveys/ ;
echo "hitid	hittypeid" > temp.successfile
FILES=./surveys/*.question

for f in $FILES
do
	mv $f ./external_hit.question
	./run.sh
	tail -n +2 external_hit.success >> temp.successfile
done
mv temp.successfile external_hit.success
